{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "description" : "arrays",
  "type" : "object",
  "properties" : {
    "minNumber" : {
      "type" : "number",
      "minimum" : 10
    },
    "maxNumber" : {
      "type" : "number",
      "maximum" : 100
    },
    "numberRange" : {
      "type" : "number",
      "minimum" : 10.2,
      "maximum" : 100.1
    },
    "minNumberEx" : {
      "type" : "number",
      "minimum": 10,
      "exclusiveMinimum" : true
    },
    "maxNumberEx" : {
      "type" : "number",
      "maximum" : 100,
      "exclusiveMaximum" : true
    },
    "numberRangeEx" : {
      "type" : "number",
      "minimum" : 10,
      "maximum" : 100,
      "exclusiveMinimum" : true,
      "exclusiveMaximum" : true
    }    
  }
}