{"$schema":"http://json-schema.org/draft-04/schema#","additionalProperties":false,"description":"CMIP6 PID Kernel information Profile for dataset files","properties":{"AGGREGATION_LEVEL":{"const":"FILE","description":"data object type, dataset or file","type":"string"},"CHECKSUM":{"description":"checksum of dataset file","pattern":"^((sha|SHA)256( |:|=))?( )*([0-9]|[a-f]){64}$","type":"string"},"CHECKSUM_METHOD":{"const":"SHA256","description":"SHA256","type":"string"},"FILE_NAME":{"description":"name of file","type":"string"},"FILE_SIZE":{"description":"size of file in bytes","type":"string"},"FILE_VERSION":{"description":"dataset file version: YYYYMMDD or 1","type":"string"},"FIXED_CONTENT":{"const":"TRUE","description":"Indicates that file will stay bit-equal, true or false","type":"string"},"HS_ADMIN":{"description":"admin information, JSON-ish","type":"string"},"IS_PART_OF":{"description":"One dataset or a list as handle id","type":"string"},"PROBLEM":{"description":"problem description - if any, optional","type":"string"},"URL":{"description":"handle id, landing page URL","type":"string"},"URL_ORIGINAL_DATA":{"description":"node information for download of original data","type":"string"}},"required":["URL","AGGREGATION_LEVEL","FIXED_CONTENT","FILE_NAME","FILE_SIZE","IS_PART_OF","FILE_VERSION","CHECKSUM","CHECKSUM_METHOD","URL_ORIGINAL_DATA","HS_ADMIN"],"type":"object"}