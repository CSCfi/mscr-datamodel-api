{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "CMIP6 Handle records - more than one entry",
    "properties": {
        "records": {
            "items": {
                "additionalProperties": false,
                "properties": {
                    "data": {
                        "additionalProperties": false,
                        "properties": {
                            "format": {
                                "description": "format of Handle data label, e.g. string, number ....",
                                "title": "format of Handle data label",
                                "type": "string"
                            },
                            "value": {
                                "title": "value of Handle data label",
                                "type": "string"
                            }
                        },
                        "required": [
                            "format",
                            "value"
                        ],
                        "title": "CMIP6 Handle data",
                        "type": "object"
                    },
                    "index": {
                        "title": "sequence number",
                        "type": "integer"
                    },
                    "timestamp": {
                        "type": "string"
                    },
                    "ttl": {
                        "const": 86400,
                        "title": "Handle time to live",
                        "type": "integer"
                    },
                    "type": {
                        "enum": [
                            "URL",
                            "AGGREGATION_LEVEL",
                            "FIXED_CONTENT",
                            "DRS_ID",
                            "HAS_PARTS",
                            "VERSION_NUMBER",
                            "REPLACES",
                            "REPLACED_BY",
                            "ERRATA_IDS",
                            "_REMOVED_ERRATA_IDS",
                            "_ADDED_ERRATA_IDS",
                            "REPLICA_NODE",
                            "HOSTING_NODE",
                            "UNPUBLISHED_REPLICAS",
                            "_REMOVED_HOSTS",
                            "HS_ADMIN",
                            "FILE_NAME",
                            "FILE_SIZE",
                            "IS_PART_OF",
                            "FILE_VERSION",
                            "CHECKSUM",
                            "CHECKSUM_METHOD",
                            "URL_ORIGINAL_DATA",
                            "PROBLEM",
                            "URL_REPLICA",
                            "LONG_TIME_ARCHIVE_URL"
                        ],
                        "title": "CMIP6 Handle key",
                        "type": "string"
                    }
                },
                "required": [
                    "index",
                    "type",
                    "data",
                    "ttl",
                    "timestamp"
                ],
                "type": "object"
            },
            "title": "CMIP6HandleRecs",
            "type": "array"
        }
    }
   
}