<https://gcmd.earthdata.nasa.gov/kms/concept/5816a48b-5200-45c0-b889-67f4a7156276>
  a skos:Concept ;
  skos:inScheme <https://gcmd.earthdata.nasa.gov/kms/concepts/concept_scheme/providers> ;
