@prefix fno: <https://w3id.org/function/ontology#> .
@prefix fnoi: <https://w3id.org/function/vocabulary/implementation#> .
@prefix fnom:    <https://w3id.org/function/vocabulary/mapping#> .
@prefix mscr: <http://uri.suomi.fi/datamodel/ns/mscr#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

mscr:simpleMappingProblem
    a fno:Problem ;
    dct:description "Handles problem of simple one to one mappings with various primitive data types" .

mscr:simpleValueTranformationProblem
    a fno:Problem ;
    dct:description "Handles problem of simple one to one mappings with value transformations" .

mscr:complexMappingTranformationProblem
    a fno:Problem ;
    dct:description "Handles problem of complex mappings" .
    
    
mscr:stringToStringFunc
    a   fno:Function ;
    fno:solves mscr:simpleMappingProblem ;
    fno:name "String passthrough";
    fno:expects ( mscr:stringInputParam );
    fno:returns ( mscr:stringOutput) .

mscr:stringToIntFunc
    a   fno:Function ;
    fno:solves mscr:simpleMappingProblem ;
    fno:name "String to integer";
    fno:expects ( mscr:stringInputParam );
    fno:returns ( mscr:intOutput) .

mscr:stringToDoubleFunc
    a   fno:Function ;
    fno:solves mscr:simpleMappingProblem ;
    fno:name "stringToDouble";
    fno:expects ( mscr:stringInputParam );
    fno:returns ( mscr:doubleOutput) .

mscr:celsiusToFahrenheitFunc
    a   fno:Function ;
    fno:solves mscr:simpleValueTranformationProblem ;
    fno:name "Celsius (double) to Fahrenheit (double)";
    fno:expects ( mscr:doubleInputParam );
    fno:returns ( mscr:doubleOutput) .
    
mscr:concatFunc
    a   fno:Function ;
    fno:solves mscr:simpleValueTranformationProblem ;
    fno:name "concat";
    fno:expects ( mscr:stringInputParam mscr:delimiterParam );
    fno:returns ( mscr:stringOutput) .
 
mscr:splitFunc
    a   fno:Function ;
    fno:solves mscr:simpleValueTranformationProblem ;
    fno:name "split";
    fno:expects ( mscr:stringInputParam mscr:delimiterParam );
    fno:returns ( mscr:stringOutput) .
     
mscr:constantFunc
    a   fno:Function ;
    fno:solves mscr:simpleValueTranformationProblem ;
    fno:name "constant";
    fno:expects ( mscr:stringInputParam mscr:constantStringParam);
    fno:returns ( mscr:stringOutput) .
  
mscr:filterFunc
    a   fno:Function ;
    fno:solves mscr:simpleValueTranformationProblem ;
    fno:name "filter";
    fno:expects ( mscr:stringInputParam mscr:propertyNameParam mscr:propertyValueParam);
    fno:returns ( mscr:stringOutput) .
          
 
mscr:customSourceResourceFunc
    a   fno:Function ;
    fno:solves mscr:simpleValueTranformationProblem ;
    fno:name "custom source resource";
    fno:expects ( mscr:stringInputParam mscr:templateParam mscr:templateDataParam mscr:templateData2Param );
    fno:returns ( mscr:stringOutput) .
          
mscr:templateParam
    a fno:Parameter ;
    fno:name "template" ;
    fno:predicate mscr:inputString ;
    fno:type xsd:string ;
    fno:required "true"^^xsd:boolean .     
    
mscr:templateDataParam
    a fno:Parameter ;
    fno:name "templateData" ;
    fno:predicate mscr:inputString ;
    fno:type xsd:string ;
    fno:required "false"^^xsd:boolean .  
    
mscr:templateData2Param
    a fno:Parameter ;
    fno:name "templateData2" ;
    fno:predicate mscr:inputString ;
    fno:type xsd:string ;
    fno:required "false"^^xsd:boolean .  
           
mscr:stringInputParam
    a fno:Parameter ;
    fno:name "input" ;
    fno:predicate mscr:inputString ;
    fno:type xsd:string ;
    fno:required "true"^^xsd:boolean .
    
mscr:constantStringParam
    a fno:Parameter ;
    fno:name "value" ;
    fno:predicate mscr:inputString ;
    fno:type xsd:string ;
    fno:required "true"^^xsd:boolean . 
    
mscr:propertyNameParam
    a fno:Parameter ;
    fno:name "property" ;
    fno:predicate mscr:inputString ;
    fno:type xsd:string ;
    fno:required "true"^^xsd:boolean .  
    
mscr:propertyValueParam
    a fno:Parameter ;
    fno:name "value" ;
    fno:predicate mscr:inputString ;
    fno:type xsd:string ;
    fno:required "true"^^xsd:boolean .       
            
mscr:delimiterParam
    a fno:Parameter ;
    fno:name "delimiter" ;
    fno:predicate mscr:inputString ;
    fno:type xsd:string ;
    fno:required "true"^^xsd:boolean .

mscr:doubleInputParam
    a fno:Parameter ;
    fno:name "input" ;
    fno:predicate mscr:inputDouble ;
    fno:type xsd:double ;
    fno:required "true"^^xsd:boolean .
        
mscr:stringOutput
    a fno:Output ;
    fno:name "string output" ;
    fno:predicate mscr:outputString ;
    fno:type xsd:string ;
    fno:required "true"^^xsd:boolean .

mscr:intOutput
    a fno:Output ;
    fno:name "int output" ;
    fno:predicate mscr:outputInt ;
    fno:type xsd:integer ;
    fno:required "true"^^xsd:boolean .
    
mscr:doubleOutput
    a fno:Output ;
    fno:name "double output" ;
    fno:predicate mscr:outputDouble ;
    fno:type xsd:double ;
    fno:required "true"^^xsd:boolean .         
    
mscr:stringToStringMapping
    a fno:Mapping ;
    fno:function mscr:stringToStringFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "stringToString" ] .

mscr:stringToIntMapping
    a fno:Mapping ;
    fno:function mscr:stringToIntFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "stringToInt" ] .

mscr:stringToDoubleMapping
    a fno:Mapping ;
    fno:function mscr:stringToDoubleFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "stringToDouble" ] .


mscr:celsiusToFahrenheitMapping
    a fno:Mapping ;
    fno:function mscr:celsiusToFahrenheitFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "celsiusToFahrenheit" ] .    
    
mscr:concatMapping
    a fno:Mapping ;
    fno:function mscr:concatFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "concat" ] .
 
mscr:splitMapping
    a fno:Mapping ;
    fno:function mscr:splitFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "split" ] .
 
mscr:constantMapping
    a fno:Mapping ;
    fno:function mscr:constantFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "constant" ] .
  
 
mscr:filterMapping
    a fno:Mapping ;
    fno:function mscr:filterFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "filter" ] .
  
mscr:customSourceResourceMapping
    a fno:Mapping ;
    fno:function mscr:customSourceResourceFunc ;
    fno:implementation mscr:simpleMappingFunctions ;
    fno:methodMapping [ a fnom:StringMethodMapping ; fnom:method-name "customSourceResource" ] .
  
 
  
mscr:simpleMappingFunctions
    a fnoi:JavaClass ;
    fnoi:class-name "fi.vm.yti.datamodel.api.v2.transformation.functions.SimpleMappingFunctions" .    